{
    "name": "thijsschalk/laravel-dismissibles",
    "description": "A Laravel package for easily handling recurring, dismissible objects like popups/notifications.",
    "version": "1.0.0",
    "type": "library",
    "license": "MIT",
    "scripts": {
        "test": "./vendor/bin/phpunit"
    },
    "autoload": {
        "psr-4": {
            "ThijsSchalk\\LaravelDismissibles\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ThijsSchalk\\LaravelDismissibles\\Tests\\": "tests"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^8.1",
        "laravel/framework": ">=7.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "ThijsSchalk\\LaravelDismissibles\\LaravelDismissiblesServiceProvider"
            ]
        }
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.54",
        "orchestra/testbench": "^9.0"
    }
}
