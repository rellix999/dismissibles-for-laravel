{
    "name": "rellix/laravel-dismissibles",
    "description": "A Laravel package for easily handling dismissible, recurring objects like popups/notifications/modals on the server side.",
    "version": "2.0.0",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "laravel",
        "dismissibles",
        "dismissible",
        "dismiss",
        "popup",
        "notification",
        "modal",
        "recurring",
        "serverside"
    ],
    "scripts": {
        "test": "./vendor/bin/phpunit",
        "test-coverage": "./vendor/bin/phpunit --coverage-text",
        "phpstan": "./vendor/bin/phpstan analyse -c phpstan.neon"
    },
    "autoload": {
        "psr-4": {
            "Rellix\\LaravelDismissibles\\": "src/",
            "Rellix\\LaravelDismissibles\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Rellix\\LaravelDismissibles\\Tests\\": "tests"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^8.1",
        "laravel/framework": ">=7.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Rellix\\LaravelDismissibles\\LaravelDismissiblesServiceProvider"
            ],
            "aliases": {
                "Dismissibles": "Rellix\\LaravelDismissibles\\Facades\\Dismissibles"
            }
        }
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.54",
        "orchestra/testbench": "^9.0",
        "phpstan/phpstan": "^1.10"
    }
}
